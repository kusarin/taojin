<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   <mapper namespace="ShopInfoChangeDao">
   <insert id="InfoChange" parameterType="cn.it.pojo.Shop">
		update into shop(userID,name,type,intro,certify) values(#{userID},#{name},#{type},#{intro},#{certify})
	</insert>
   </mapper>